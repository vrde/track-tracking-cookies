{
	"manifest_version": 2,

	"name": "Track Tracking Cookies",
	"description" : "Base Level Extension",
	"version": "0.1.0",

	"permissions": [
		"tabs",
		"storage",
		"cookies",
		"*://*/*"
	],

	"icons": {
		"16": "icons/cookie16.png",
		"48": "icons/cookie48.png",
		"128": "icons/cookie128.png"
	},

	"options_ui": {
		"page": "home.html",
		"open_in_tab": true
	},
	"browser_action": {
		"default_icon": {
			"16": "icons/cookie16.png",
			"48": "icons/cookie48.png",
			"128": "icons/cookie128.png"
		}
	},
	"background": {
		"scripts": ["background.js"]
	}
}
